            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProducerChipset)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CoreClock)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CoreClockBoost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConnectorType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resolution)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecommendedPSUPower)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmountOfRAM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypeOfRAM)
