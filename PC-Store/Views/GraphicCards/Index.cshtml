                @Html.DisplayNameFor(model => model.NumberOfFans)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DSub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisplayPort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HDMI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DVI)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Producer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProducerChipset)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CoreClock)
