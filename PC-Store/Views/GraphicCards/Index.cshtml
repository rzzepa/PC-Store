            <td>
                @Html.DisplayFor(modelItem => item.AmountOfRAM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeOfRAM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataBus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemoryClock)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CoolingType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfFans)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DSub)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisplayPort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HDMI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DVI)
